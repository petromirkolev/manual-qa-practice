          "listen": "test",
          "script": {
            "exec": [
              "const jsonData = pm.response.json();",
              "const post = pm.collectionVariables.get(\"post\");",
              "",
              "// Check response code",
              "pm.test(\"Status code is 200\", function () {",
              "    pm.response.to.have.status(200);",
              "});",
              "",
              "// Check if post matches requested post",
              "pm.test(\"Post ID matches requested ID\", function () {",
              "    pm.expect(jsonData.id).to.eql(Number(post));",
              "});"
            ],
            "type": "text/javascript",
            "packages": {},
            "requests": {}
          }
        }
