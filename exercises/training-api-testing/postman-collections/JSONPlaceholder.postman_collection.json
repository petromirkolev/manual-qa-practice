    {
      "name": "Create post",
      "event": [
        {
          "listen": "test",
          "script": {
            "exec": [
              "// Response status is 201 Created",
              "pm.test(\"Status code is 201\", function () {",
              "    pm.response.to.have.status(201);",
              "});",
              "",
              "// Response body contains the correct title",
              "pm.test(\"Response body contains the correct title\", function () {",
              "    pm.expect(pm.response.json().title).to.eql(\"hello\");",
              "});",
              "",
              "// Response body contains the correct body",
              "pm.test(\"Response body contains the correct body\", function () {",
              "    pm.expect(pm.response.json().body).to.eql(\"world\");",
              "});",
              "",
              "// Response userID is 1",
              "pm.test(\"Response userId is 1\", function () {",
              "    pm.expect(pm.response.json().userId).to.eql(1)",
              "});",
              "",
              "// Response id is greater than 100  ",
              "pm.test(\"Response id is greater than 100\", function () {",
              "    pm.expect(pm.response.json().id).to.be.greaterThan(100);",
              "});"
            ],
